    "babel-loader": "7.1.3",
    "babel-preset-latest": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chalk": "2.3.1",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.2",
    "cross-env": "5.1.3",
    "css-loader": "0.28.10",
    "eslint": "4.18.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-watch": "3.1.3",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.0.0",
